                      "2": {
                          "name": "support.constant.core.rust"
                      },
                      "3": {
                          "name": "entity.name.type.rust"
                      }
                  }
              },
              {
                  "match": "(Err|Ok|Some)(?=[(])",
                  "name": "support.constant.core.rust"
              },
              {
                  "name": "storage.type.rust",
                  "match": "impl(?=<)"
              },
              {
                  "match": "(mod)\\s([a-zA-Z_]+)",
                  "captures": {
                      "1": {
