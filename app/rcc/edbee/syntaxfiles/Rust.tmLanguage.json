              {
                  "comment": "Reserved keyword",
                  "name": "invalid.deprecated.rust",
                  "match": "\\b(abstract|alignof|become|do|final|macro|offsetof|override|priv|proc|pure|sizeof|typeof|virtual|yield)\\b"
              }
